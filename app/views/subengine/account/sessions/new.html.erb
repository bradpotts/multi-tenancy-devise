<%= link_to "New User?", subengine.user_sign_up_url %>
<h2>Sign In</h2>
<%= form_for @user, :url => sessions_url do |f| %>
  <p>
    <%= f.label :email %><br>
    <%= f.email_field :email%>
  </p>
  <p>
    <%= f.label :password %><br>
    <%= f.password_field :password%>
  </p>
  <p>
    <%= f.submit "Sign In", :class => "btn btn-primary" %>
  </p>
<% end %>